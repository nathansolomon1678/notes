Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"PROBLEM", " ", "1"}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{"Remove", "[", "\"\<Global`*\>\"", "]"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Comment", " ", "out", " ", "this", " ", "section", " ", "below", " ", 
     "to", " ", "get", " ", "symbolic", " ", "values", " ", "instead", " ", 
     "of", " ", "numeric", " ", "ones"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"m", " ", "=", " ", "1"}], ";"}], "  ", 
   RowBox[{"(*", " ", "kilogram", " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"g", " ", "=", " ", "9.82"}], ";"}], "  ", 
   RowBox[{"(*", " ", 
    RowBox[{"meters", " ", "per", " ", "second", " ", "squared"}], " ", 
    "*)"}], "\n", 
   RowBox[{
    RowBox[{"\[Alpha]", " ", "=", " ", 
     RowBox[{"34", " ", "Degree"}]}], ";"}], "  ", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"latitude", " ", "of", " ", "Los", " ", "Angeles"}], ",", " ", 
     RowBox[{"converted", " ", "to", " ", "radians"}]}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"\[Omega]", " ", "=", " ", 
     FractionBox[
      RowBox[{"2", " ", "\[Pi]"}], 
      RowBox[{"24", " ", "*", " ", "60", " ", "*", " ", "60"}]]}], ";"}], 
   "  ", 
   RowBox[{"(*", " ", 
    RowBox[{"radians", " ", "per", " ", "second"}], " ", "*)"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<Angular velocity of Earth: \[Omega] = \>\"", ",", " ", "\[Omega]", 
     ",", " ", "\"\< = \>\"", ",", " ", 
     RowBox[{"N", "[", "\[Omega]", "]"}], ",", " ", 
     "\"\< radians per second\>\""}], "]"}], "\n", 
   RowBox[{
    RowBox[{"R", " ", "=", " ", 
     RowBox[{"6.4", " ", "\[Times]", " ", 
      SuperscriptBox["10", "6"]}]}], ";"}], "  ", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"approximate", " ", "radius", " ", "of", " ", "Earth"}], ",", 
     " ", 
     RowBox[{"in", " ", "meters"}]}], " ", "*)"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Let", " ", "r", " ", "be", " ", "position"}], ",", " ", 
     RowBox[{"v", " ", "be", " ", "velocity"}], ",", " ", 
     RowBox[{
     "and", " ", "\[CapitalOmega]", " ", "be", " ", "angular", " ", 
      "velocity"}], ",", " ", 
     RowBox[{"in", " ", "the", " ", 
      RowBox[{"{", 
       RowBox[{"x", ",", " ", "y", ",", " ", "z"}], "}"}], " ", "coordinate", 
      " ", "system"}]}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"r", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{"0", ",", " ", "0", ",", " ", "0"}], "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"v", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{"0", ",", " ", "0", ",", " ", "0"}], "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"\[CapitalOmega]", " ", ":=", " ", 
     RowBox[{"{", 
      RowBox[{"0", ",", " ", 
       RowBox[{
        RowBox[{"-", " ", "\[Omega]"}], " ", 
        RowBox[{"Cos", "[", "\[Alpha]", "]"}]}], ",", " ", 
       RowBox[{"\[Omega]", " ", 
        RowBox[{"Sin", "[", "\[Alpha]", "]"}]}]}], "}"}]}], ";"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"WARNING", ":", " ", 
     RowBox[{
     "the", " ", "symbol", " ", "for", " ", "cross", " ", "product", " ", 
      RowBox[{"(", 
       RowBox[{"\[Cross]", 
        RowBox[{",", " ", 
         RowBox[{"`Esc", " ", "cross", " ", "Esc`"}]}]}], ")"}], " ", "is", 
      " ", "different", " ", "from", " ", "the", " ", "symbol", " ", "for", 
      " ", "\"\<times\>\"", " ", 
      RowBox[{"(", 
       RowBox[{"\[Times]", 
        RowBox[{",", " ", 
         RowBox[{"`Esc", " ", "*", " ", "Esc`"}]}]}], ")"}]}]}], " ", "*)"}], 
   "\n", 
   RowBox[{"GravitationalForce", " ", ":=", " ", 
    RowBox[{"MatrixForm", "[", 
     RowBox[{"N", "[", 
      RowBox[{"{", 
       RowBox[{"0", ",", " ", "0", ",", " ", 
        RowBox[{
         RowBox[{"-", " ", "m"}], " ", "g"}]}], "}"}], "]"}], "]"}]}], "\n", 
   RowBox[{"CentrifugalForce", " ", ":=", " ", 
    RowBox[{"MatrixForm", "[", 
     RowBox[{"Simplify", "[", 
      RowBox[{"N", "[", 
       RowBox[{
        RowBox[{"-", "m"}], " ", 
        RowBox[{"\[CapitalOmega]", " ", "\[Cross]", " ", 
         RowBox[{"(", 
          RowBox[{"\[CapitalOmega]", " ", "\[Cross]", " ", 
           RowBox[{"(", 
            RowBox[{"r", " ", "+", " ", 
             RowBox[{"{", 
              RowBox[{"0", ",", " ", "0", ",", " ", "R"}], "}"}]}], ")"}]}], 
          ")"}]}]}], "]"}], "]"}], "]"}]}], "\n", 
   RowBox[{"CoriolisForce", " ", ":=", " ", 
    RowBox[{"MatrixForm", "[", 
     RowBox[{"N", "[", 
      RowBox[{
       RowBox[{"-", "2"}], " ", "m", " ", 
       RowBox[{"\[CapitalOmega]", " ", "\[Cross]", " ", "v"}]}], "]"}], 
     "]"}]}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<Gravitational \\\"force\\\": \>\"", ",", " ", "GravitationalForce", 
     ",", " ", "\"\< Newtons\>\""}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<Centrifugal \\\"force\\\": \>\"", ",", " ", "CentrifugalForce", ",", 
     " ", "\"\< Newtons\>\""}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<Coriolis \\\"force\\\": \>\"", ",", " ", "CoriolisForce", ",", " ", 
     "\"\< Newtons\>\""}], "]"}], "\n", "\n", 
   RowBox[{
    RowBox[{"v", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{"0", ",", " ", "30", ",", " ", "0"}], "}"}]}], ";"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<Now make the mass move 30 m/s south. Gravitational and centrifugal \
\\\"forces\\\" are unchanged, but now Coriolis \\\"force\\\" is \>\"", ",", 
     "\n", "\t", 
     RowBox[{"CoriolisForce", " ", "//", " ", "MatrixForm"}], ",", " ", 
     "\"\< Newtons\>\""}], "]"}], "\n", "\n", 
   RowBox[{"Clear", "[", "\[Alpha]", "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<Is centrifugal \\\"force\\\" negligible? In general, it's equal to \
\>\"", ",", " ", "CentrifugalForce", ",", " ", 
     "\"\< \!\(\*FractionBox[\(\(meter\)\(\\\ \)\), \
SuperscriptBox[\(second\), \(2\)]]\) times the mass\>\""}], "]"}], "\n", 
   RowBox[{"MaxCentrifugalForce", " ", ":=", " ", 
    RowBox[{"m", " ", "R", " ", 
     SuperscriptBox["\[Omega]", "2"]}]}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<That means the magnitude of the centrifugal \\\"force\\\" is at most \
\>\"", ",", " ", "MaxCentrifugalForce", ",", " ", 
     "\"\< \!\(\*FractionBox[\(\(meter\)\(\\\ \)\), \
SuperscriptBox[\(second\), \(2\)]]\) times the mass\>\""}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<That's \>\"", ",", " ", 
     RowBox[{"100", " ", 
      FractionBox["MaxCentrifugalForce", "g"]}], ",", " ", 
     "\"\<% of the magnitude of the gravitational force\>\""}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<Also, the y component of the centrifugal \\\"force\\\" could be as \
much as \>\"", ",", " ", 
     FractionBox["MaxCentrifugalForce", "2"], ",", "\n", "\t", 
     "\"\< \!\(\*FractionBox[\(\(meter\)\(\\\ \)\), \
SuperscriptBox[\(second\), \(2\)]]\) times the mass (\>\"", ",", " ", 
     RowBox[{"50", " ", 
      FractionBox["MaxCentrifugalForce", "g"]}], ",", " ", 
     "\"\<% of g)\>\""}], "]"}], "\n", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<So, whether the centrifugal effect is negligible really depends on \
the application. If you're building a pendulum clock or something, \>\"", 
     "\n", "\t", 
     "\"\<you should account for it, but if you're assuming g=10m/s for a \
simple physics problem, then don't bother.\>\""}], "]"}]}]}]], "Code",
